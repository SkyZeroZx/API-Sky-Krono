{
  "name": "sky-krono-api",
  "version": "1.0.0",
  "description": "Awesome API Krono NestJS",
  "author": "SkyZeroZx",
  "private": true,
  "engines": {
    "node": "16.16.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "test:e2e:watch": "jest --config ./e2e/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./e2e/jest-e2e.json --coverage --forceExit",
    "sonar": "sonar-scanner"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/typeorm": "^9.0.1",
    "@simplewebauthn/server": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.7",
    "dotenv": "^16.0.1",
    "generate-password": "^1.7.0",
    "helmet": "5.0.2",
    "ioredis": "^5.2.3",
    "mysql2": "^2.3.3",
    "nest-access-control": "^2.2.0",
    "nest-winston": "^1.7.0",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^16.0.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.10",
    "web-push": "^3.5.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.2",
    "@types/cors": "^2.8.6",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "jest-sonar": "^0.2.12",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.2",
    "sonar-scanner": "^3.1.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "jest": {
    "testTimeout": 15000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\**.(controller|service|guard|decorator|strategy).spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testResultsProcessor": "jest-sonar-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.dto.(t|j)s",
      "!**/*.entity.(t|j)s",
      "!**/**.mock.spec.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/*.swagger.(t|j)s",
      "!**/*.enum.(t|j)s",
      "!**/common/*.(t|j)s",
      "!**/config/*.(t|j)s",
      "!**common/helpers/*.(t|j)s",
      "!**common/swagger/**.(t|j)s",
      "!**main.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
